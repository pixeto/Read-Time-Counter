window.onload=function(){rtCounter()};function rtCounter(){var e=document.getElementById("readtimearea"),t=e.innerHTML,n=t=t.replace(/[\u007F-\u00FE]/g,""),r=t;n=n.replace(/[^!-~\d\s]+/gi,""),r=r.replace(/[!-~\d\s]+/gi,"");var a=n.match(/[\u00ff-\uffff]|\S+/g),g=r.match(/[\u00ff-\uffff]|\S+/g),$=((count1=a?a.length:0)+.85*(count2=g?g.length:0)).toFixed(0),d=e.getElementsByTagName("img"),f=count1/235+count2/280+20*d.length/60;document.getElementById("readtime").innerHTML=f.toFixed(1),document.getElementById("hybridCount").innerHTML="( around "+$+" words and "+d.length+" images )"}
